function readdirSync (dir, options, internalOptions) {
  internalOptions.facade = syncFacade;

  let reader = new directoryReader(dir, options, internalOptions);
  let stream = reader.stream;

  let results = [];
  let data = stream.read();
  while (data !== null) {
    results.push(data);
    data = stream.read();
  }

  return results;
}

