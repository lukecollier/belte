  if (tokens.matches1AtIndex(index, TokenType$2.name)) {
    importedNames.add(tokens.identifierNameAtIndex(index));
    index++;
    if (tokens.matches1AtIndex(index, TokenType$2.comma)) {
      index++;
    }
  }

  if (tokens.matches1AtIndex(index, TokenType$2.star)) {
    // * as
    index += 2;
