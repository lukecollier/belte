function startTagAfterHead(p, token) {
    const tn = token.tagName;

    if (tn === $$6.HTML) {
        startTagInBody(p, token);
    } else if (tn === $$6.BODY) {
        p._insertElement(token, NS$3.HTML);
        p.framesetOk = false;
        p.insertionMode = IN_BODY_MODE;
    } else if (tn === $$6.FRAMESET) {
        p._insertElement(token, NS$3.HTML);
        p.insertionMode = IN_FRAMESET_MODE;
    } else if (
        tn === $$6.BASE ||
        tn === $$6.BASEFONT ||
        tn === $$6.BGSOUND ||
        tn === $$6.LINK ||
        tn === $$6.META ||
        tn === $$6.NOFRAMES ||
        tn === $$6.SCRIPT ||
        tn === $$6.STYLE ||
        tn === $$6.TEMPLATE ||
        tn === $$6.TITLE
