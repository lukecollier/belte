function aliasFromSource(src) {
  const filename = filenameFromPath(src);
  if (filename === 'index.js') {
    const index = src.lastIndexOf('/index.js');
    const slash = src.substring(0,index).lastIndexOf('/');
    return src.substring(slash+1, index);
  } else {
    return src;
  }
}

